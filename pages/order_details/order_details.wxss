page {
    font-size: 28rpx;
    color: #333;
    background-color: #efeff4;
}

.block {
    position: relative;
    margin: 25rpx 0;
    background-color: #fff;
    padding-left: 30rpx;
}

.top {
    padding: 30rpx;
}

.top_r {
    float: right;
    color: #ffda44;
}

.arrows {
    position: absolute;
    right: 50rpx;
    top: 50%;
    width: 28rpx;
    height: 28rpx;
    border-width: 1rpx;
    border-color: #999 #999 transparent transparent;
    border-style: solid;
    transform: rotate(45deg) translateY(-50%);
}

.box-align-center {
    -webkit-box-align: center;
    -moz-box-align: center;
}

.display-box-h {
    width: 100%;
    display: box;
    box-orient: horizontal;
}

.flex-one {
    box-flex: 1;
}

.masterInfoCon {
    position: relative;
    padding: 30rpx;
}

.headerImgCon {
    width: 100rpx;
    height: 100rpx;
    border-radius: 100%;
    overflow: hidden;
    background-color: #999;
}

.headerImgCon image {
    width: 100%;
    height: 100%;
}

.masterInfo {
    padding-left: 30rpx;
    padding-right: 30rpx;
}

.masterPhone {
    width: 60rpx;
    height: 60rpx;
    border-radius: 100%;
    padding: 15rpx;
    background-color: #89ca12;
}

.masterPhone image {
    width: 100%;
    height: 100%;
}

.masterInfo_top {
    font-size: 32rpx;
    margin-bottom: 10rpx;
}

.masterInfo_tip {
    color: #999;
    font-size: 28rpx;
}

.serverInfoCon {
    padding: 15rpx 30rpx 15rpx 30rpx;
}

.serverInfoCon>view {
    padding: 30rpx 0;
}

.serverInfoCon>view view {
    display: inline-block;
    color: #666;
}

.serverInfo_l {
    float: left;
    margin-right: -160rpx;
    font-weight: 700;
    color: #333;
    width: 160rpx;
}

.serverInfo_r {
    padding-left: 160rpx;
}

.title {
    padding: 40rpx 0;
    border-bottom: 1rpx solid #e4e4e4;
}

.title text {
    color: #333;
    font-size: 32rpx;
    font-weight: 700;
    margin-right: 10rpx;
}

.defalutCon {
    padding: 15rpx 0;
    padding-right: 30rpx;
    border-bottom: 1rpx solid #e4e4e4;
}

.defalutCon_row {
    padding: 5rpx 0;
}

.defalutCon_row text {
    font-size: 28rpx;
    color: #666;
}

.defalutCon_row_r {
    float: right;
    color: #333!important;
    font-weight: 700;
    margin-top: 10rpx;
}

.defalutImgCon {
    padding: 30rpx 0;
}

.defalutImgCon image {
    width: 100rpx;
    height: 100rpx;
    border: 1rpx solid #e4e4e4;
    margin-right: 20rpx;
}

.youCon {
    padding: 30rpx;
}

.youCon text:first-child {
    font-weight: 700;
}

.youCon_text {
    float: right;
    color: #666;
}

.priceCon {
    padding: 30rpx;
}

.priceCon view {
    padding: 10rpx 0;
    position: relative;
}

.priceCon_r {
    position: absolute;
    right: 0;
    font-weight: 700;
}

.priceCon_tip {
    font-size: 24rpx;
    color: #999;
}

.leaveWord,.orderInfo {
    padding: 30rpx;
}

.orderInfo>view:first-child {
    margin-bottom: 30rpx;
}

.row_l {
    float: left;
    display: block;
    width: 160rpx;
    font-weight: 700;
    margin-right: -160rpx;
}

.row_r {
    display: block;
    padding-left: 160rpx;
    color: #666;
    word-break: break-all;
    word-wrap: break-word;
}

.payCon {
    padding: 0 30rpx;
    background-color: #fff;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    border-top: 1px solid #e4e4e4;
}

.payCon>text {
    float: right;
    padding: 15rpx 20rpx;
    border: 1rpx solid #e4e4e4;
    border-radius: 8rpx;
    margin: 20rpx 0 20rpx 20rpx;
}

.payCon>text:active {
    background-color: #f2f2f2;
}

.pay {
    border: none!important;
    background-color: #ffda44;
}

.pay:active {
    background-color: #f0da44;
}

.apply_invoiceBg,.cancleOrderBg {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 9;
    background-color: rgba(0,0,0,0.5);
}

.apply_invoiceCon,.cancleOrderCon {
    position: absolute;
    left: 50%;
    top: 50%;
    width: 90%;
    transform: translateX(-50%) translateY(-50%);
    background-color: #fff;
    border-radius: 4px;
}

.apply_invoice_title {
    text-align: center;
    font-size: 16px;
    border-bottom: 1px solid #e4e4e4;
    padding: 15px;
}

.apply_invoice_inputCon {
    padding: 15px;
}

.apply_invoice_inputCon>text {
    float: left;
    height: 30px;
    line-height: 30px;
}

.apply_invoice_inputCon>input {
    width: 70%;
    height: 30px;
    line-height: 30px;
    padding-left: 10px;
    margin-left: 15px;
    border: 1px solid #e4e4e4;
    float: left;
}

.apply_invoice_btnCon {
    border-top: 1px solid #e4e4e4;
}

.apply_invoice_btnCon>text {
    float: left;
    font-size: 16px;
    width: 50%;
    height: 40px;
    line-height: 40px;
    text-align: center;
    float: left;
    box-sizing: border-box;
}

.apply_invoice_btnCon>text:first-child {
    border-right: 1px solid #e4e4e4;
}

.cancleOrder_t {
    text-align: center;
    padding: 30rpx 0 10rpx;
    font-size: 32rpx;
}

.cancleOrder_t1 {
    text-align: center;
    padding-bottom: 20rpx;
    border-bottom: 1rpx solid #e4e4e4;
}

.cancleOrder_row {
    padding: 10rpx 30rpx;
}

.cancleOrder_row text {
    display: inline-block;
    vertical-align: middle;
}

.cancleOrder_row_l {
    position: relative;
    width: 40rpx;
    height: 40rpx;
    border-radius: 100%;
    background-color: rgba(0,0,0,0.5);
    margin-right: 20rpx;
}

.cancleOrder_row_l::before {
    position: absolute;
    content: "";
    left: 8rpx;
    top: 10rpx;
    width: 20rpx;
    height: 10rpx;
    border-style: solid;
    border-width: 1rpx;
    border-color: transparent transparent #fff #fff;
    transform: rotate(-45deg);
}

.cancleOrder_row_l_active {
    background-color: #ffda44;
}

.cancleTextarea textarea {
    width: 80%;
    height: 160rpx;
    border: 1rpx solid #e4e4e4;
    border-radius: 8rpx;
    padding: 20rpx;
    margin: 0 auto 20rpx;
}

.cancleBtn {
    text-align: center;
    margin-bottom: 30rpx;
}

.cancleBtn text {
    display: inline-block;
    padding: 20rpx 60rpx;
    border: 1rpx solid #e4e4e4;
    border-radius: 8rpx;
}

.cancleBtn text:last-child {
    color: #fff;
    border: 1rpx solid #ffda44;
    background-color: #ffda44;
    margin-left: 20rpx;
}

.voucher {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.3);
}

.voucher_img {
    position: relative;
    width: 95%;
    height: 100%;
    margin: 20px auto 0;
    overflow: hidden;
    overflow-y: auto;
}

.voucherImg {
    display: block;
    width: 100%;
}

.voucher_close {
    position: absolute;
    top: 16px;
    right: 16px;
    width: 32px;
    height: 32px;
}