page {
    font-size: 28rpx;
}

.rowCon {
    padding: 10rpx 0 10rpx 30rpx;
    background-color: #fff;
    margin-bottom: 20rpx;
}

.row {
    position: relative;
    padding: 10rpx;
}

.row1 {
    padding: 20rpx 0;
}

.row::after {
    clear: both;
    content: "";
    display: block;
    height: 0px;
    visibility: hidden;
}

.row_l {
    float: left;
    width: 160rpx;
    margin-right: -160rpx;
}

.row_r {
    padding-left: 160rpx;
    color: #666;
}

.row_name {
    padding-bottom: 20rpx;
}

.row_address {
    padding-right: 50rpx;
}

.arrows {
    position: absolute;
    right: 40rpx;
    top: 50%;
    width: 20rpx;
    height: 20rpx;
    border-width: 1rpx;
    border-color: #999 #999 transparent transparent;
    border-style: solid;
    transform: rotate(45deg) translateY(-50%);
}

.algin_r {
    text-align: right;
    padding-right: 60rpx;
}

.textarea {
    display: block;
    width: 100%;
    height: 200rpx;
    padding: 30rpx;
    margin-bottom: 20rpx;
    background-color: #fff;
}

.placeholder {
    color: #999;
}

.confirm {
    width: 90%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 36rpx;
    border-radius: 8rpx;
    color: #333;
    background-color: #ffda44;
    margin: auto;
}

.confirm:active {
    background-color: #f0da44;
}

.imgCon {
    padding: 30rpx;
    margin: 30rpx auto;
    background-color: #fff;
}

.imgCon view {
    position: relative;
    display: inline-block;
    vertical-align: top;
    width: 100rpx;
    height: 100rpx;
    text-align: center;
    border: 1rpx solid #e4e4e4;
    margin-right: 30rpx;
}

.imgCon view image {
    width: 100%;
    height: 100%;
}

.imgCon view text {
    position: absolute;
    right: -20rpx;
    top: -20rpx;
    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    border-radius: 100%;
    color: #fff;
    background-color: #ccc;
}

.addImg {
    line-height: 86rpx;
    font-size: 80rpx;
    font-weight: 700;
    color: #cccccc;
}

.selectCon {
    position: relative;
    height: 142rpx;
    line-height: 142rpx;
    padding-right: 30rpx;
}

.selectCon image,.selectCon text {
    display: inline-block;
    vertical-align: middle;
}

.selectCon image {
    width: 46rpx;
    height: 46rpx;
    margin-right: 20rpx;
}

.selectCon .text {
    color: #c7c7c7;
}

.addressCon {
    border-bottom: 1rpx solid #e4e4e4;
}

.arrows {
    position: absolute;
    right: 50rpx;
    top: 50%;
    width: 28rpx;
    height: 28rpx;
    border-width: 1rpx;
    border-color: #999 #999 transparent transparent;
    border-style: solid;
    transform: rotate(45deg) translateY(-50%);
}

.timeShade {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background-color: rgba(0,0,0,0.5);
    z-index: 3;
}

.timeSelectCon {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 500rpx;
    overflow: hidden;
    background-color: #fff;
}

.timeSelect_l {
    float: left;
    width: 40%;
    height: 400rpx;
    text-align: center;
    overflow-y: auto;
    background-color: #f2f2f2;
}

.timeSelect_l>view,.timeSelect_r>view {
    line-height: 80rpx;
    color: #999;
}

.timeSelect_l>view.active {
    color: #333;
    background-color: #fff;
}

.timeSelect_r>view.active {
    color: #ffda44;
}

.timeSelect_r {
    float: left;
    width: 60%;
    height: 400rpx;
    text-align: center;
    overflow-y: auto;
}

.timeSelect_btn {
    height: 100rpx;
    line-height: 100rpx;
    font-size: 32rpx;
    text-align: center;
    border-top: 1rpx solid #e4e4e4;
}

.timeSelect_btn:active {
    background-color: #f2f2f2;
}

.faultListBG {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background-color: rgba(0,0,0,0.5);
    z-index: 999;
}

.faultListCon {
    position: absolute;
    width: 90%;
    left: 50%;
    top: 50%;
    max-height: 80%;
    transform: translateX(-50%) translateY(-50%);
    padding: 28rpx;
    border-radius: 8rpx;
    background-color: #fff;
}

.faultList_detailCon {
    max-height: 550rpx;
    overflow-y: scroll;
}

.faultListCon_title {
    text-align: center;
    font-size: 32rpx;
    margin-bottom: 20rpx;
}

.faultListRow {
    padding: 24rpx 0;
    border-bottom: 1px solid #e4e4e4;
}

.faultList_detailCon>view:last-child {
    border-bottom: none;
}

.faultListRow>view {
    font-size: 28rpx;
}

.faultListRow_l {
    float: left;
}

.faultListRow_r {
    float: right;
    color: #ffda44;
}

.faultListClose {
    position: absolute;
    right: -30rpx;
    top: -30rpx;
    width: 60rpx;
    height: 60rpx;
}

.guzhang_alertBox {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background-color: rgba(0,0,0,0.5);
    z-index: 4;
}

.guzhang_alertBox .alert_conBox {
    width: 640rpx;
    min-height: 200rpx;
    padding: 4%;
    background-color: white;
    position: fixed;
    top: 49%;
    left: 50%;
    margin-left: -320rpx;
    border-radius: 8px;
    transform: translateY(-50%);
}

.guzhang_alertBox .alert_conBox .title {
    text-align: center;
    font-size: 36rpx;
    color: #333;
    line-height: 50rpx;
}

.guzhang_alertBox .alert_conBox .title02 {
    text-align: center;
    font-size: 28rpx;
    color: #999;
    margin-bottom: 4%;
}

.guzhang_alertBox .alert_conBox .alert_con {
    width: 100%;
    font-size: 28rpx;
    border: 1px solid #efeff4;
    border-radius: 4px;
    line-height: 80rpx;
    padding: 0 5%;
    display: flex;
    margin-bottom: 3%;
}

.guzhang_alertBox .alert_conBox .alert_con.on {
    background-color: #fff4c7;
    border-color: #ffeda4;
}

.guzhang_alertBox .alert_conBox .alert_con .view01 {
    width: 77%;
}

.guzhang_alertBox .alert_conBox .alert_con .view02 {
    width: 23%;
    display: flex;
    justify-content: flex-end;
}

.guzhang_alertBox .alert_conBox .alert_bottonBox {
    width: 100%;
    display: flex;
    justify-content: space-between;
    margin-top: 6%;
}

.guzhang_alertBox .alert_conBox .alert_bottonBox .left {
    width: 48%;
}

.guzhang_alertBox .alert_conBox .alert_bottonBox .right {
    width: 48%;
}

button[type="default"] {
    color: #999;
    background-color: white;
}

button[type="primary"] {
    color: #333;
    background-color: #ffda44;
}

.guzhang_alertBox .right .button-hover {
    background-color: #ffda44;
}

.formBox {
    font-size: 28rpx;
    position: relative;
}

.formBox .img {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    top: 50%;
    left: 0;
    transform: translateY(-50%);
    z-index: 2;
}

.formBox i {
    width: 16rpx;
    height: 16rpx;
    border: 1px solid #808080;
    border-bottom: transparent;
    border-left: transparent;
    position: absolute;
    right: 3%;
    top: 50%;
    transform: translateY(-50%) rotate(45deg);
}

.formBox .text {
    position: relative;
    border-bottom: 1px solid #f4f4f4;
    padding: 4% 0 4% 6%;
}

.openSettingBtn {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 1;
    opacity: 0;
}

.block {
    padding: 0 3% 0 0;
}