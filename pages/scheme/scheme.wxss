image {
    overflow: inherit;
}

.topCon {
    margin-bottom: 20rpx;
}

.top_price {
    padding: 52rpx 0;
    text-align: center;
    background-color: #ffda44;
}

.top_price_tip {
    font-size: 24rpx;
}

.top_price_icon {
    font-size: 24rpx;
}

.top_price_num {
    font-size: 60rpx;
    font-weight: 700;
}

.top_btnCon {
    padding: 24rpx 0;
    background-color: #fff;
}

.top_btnCon>view {
    float: left;
    width: 50%;
    font-size: 32rpx;
    text-align: center;
}

.top_btnCon>view text {
    display: inline-block;
    vertical-align: middle;
}

.top_btnCon>view image {
    display: inline-block;
    vertical-align: middle;
    width: 32rpx;
    height: 32rpx;
    margin-right: 12rpx;
}

.top_btnCon>view:first-child {
    border-right: 1px solid #e4e4e4;
}

.modelBox {
    padding: 2% 3% 0;
    background-color: white;
    display: flex;
    border-bottom: 1px solid #f4f4f4;
}

.modelBox .img {
    width: 22%;
    text-align: center;
}

.modelBox .img image {
    width: 152rpx;
    height: 152rpx;
    margin-bottom: 20rpx;
}

.modelBox .text {
    width: 78%;
    font-size: 38rpx;
    font-weight: 600;
    line-height: 140rpx;
    padding-left: 20rpx;
}

.detailBox {
    padding: 0% 3% 0;
    background-color: white;
    border-bottom: 1px solid #f4f4f4;
}

.detail {
    padding-top: 2%;
    padding-bottom: 2%;
    border-bottom: 1px solid #f4f4f4;
}

.detail:last-of-type {
    border-bottom: none;
}

.detail .text01 {
    display: flex;
    color: #333;
    font-size: 28rpx;
    line-height: 50rpx;
}

.detail .text01 .type {
    width: 50%;
}

.detail .text01 .price {
    width: 50%;
    display: flex;
    justify-content: flex-end;
}

.detail .text02 {
    font-size: 24rpx;
    color: #999;
}

.all_price {
    background-color: white;
    display: flex;
    justify-content: flex-end;
    font-size: 32rpx;
    color: #333;
    line-height: 90rpx;
    padding: 0 3%;
}

.modeTitle {
    background-color: white;
    margin-top: 3%;
    line-height: 80rpx;
    font-size: 32rpx;
    text-align: center;
}

.modeBox {
    display: flex;
    background-color: white;
    padding: 1% 0 3%;
}

.modeBox .mode {
    width: 25%;
    padding: 0 3%;
    font-size: 28rpx;
    border-right: 1px solid #f4f4f4;
    position: relative;
}

.modeBox .mode i {
    position: absolute;
    bottom: -26rpx;
    left: 50%;
    transform: translateX(-50%);
    width: 0;
    height: 0;
    border-left: 12rpx solid transparent;
    border-right: 12rpx solid transparent;
    border-bottom: 18rpx solid #ffeca1;
    display: none;
}

.modeBox .mode.on i {
    display: block;
}

.modeBox .mode:last-of-type {
    border-right: none;
}

.modeBox .mode .img {
    width: 60rpx;
    height: 60rpx;
}

.modeBox .mode view {
    text-align: center;
    color: #808080;
}

.main_title {
    text-align: center;
    font-size: 28rpx;
    color: #dda14e;
    background-color: #ffeca1;
    line-height: 64rpx;
}

.bg-w {
    background-color: white;
}

.formBox {
    margin-left: 3%;
    font-size: 28rpx;
    position: relative;
}

.formBox .img {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    top: 50%;
    left: 0;
    transform: translateY(-50%);
    z-index: 2;
}

.formBox i {
    width: 16rpx;
    height: 16rpx;
    border: 1px solid #808080;
    border-bottom: transparent;
    border-left: transparent;
    position: absolute;
    right: 3%;
    top: 50%;
    transform: translateY(-50%) rotate(45deg);
}

.formBox .text {
    position: relative;
    border-bottom: 1px solid #f4f4f4;
    padding: 4% 0 4% 6%;
}

.formBox .determine {
    position: absolute;
    right: 3%;
    top: 50%;
    transform: translateY(-50%);
    padding: 10rpx 20rpx;
    border-radius: 4px;
    font-size: 28rpx;
    background-color: #FAD502;
}

.formBox .remark {
    width: 60rpx;
    height: 44rpx;
    line-height: 44rpx;
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
}

.formBox input {
    line-height: 44rpx;
    height: 44rpx;
}

.formBox .remark_input {
    padding-left: 40rpx;
    margin-right: 200rpx;
}

.openSettingBtn {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 1;
    opacity: 0;
}

.duihuanBtn {
    position: absolute;
    right: 30rpx;
    top: 50%;
    transform: translateY(-50%);
    border-radius: 8rpx;
    padding: 10rpx 20rpx;
}

.prompting {
    padding: 0 3%;
}

.prompting view {
    line-height: 46rpx;
    color: #999;
    font-size: 24rpx;
}

.selectColorCon {
    margin: 20rpx 0;
    padding: 30rpx;
    background-color: #fff;
}

.selectColorT {
    font-size: 32rpx;
}

.selectColorRow>text {
    float: left;
    font-size: 28rpx;
    min-width: 200rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    border: 1px solid #e4e4e4;
    border-radius: 8rpx;
    margin-top: 20rpx;
    margin-right: 20rpx;
}

.selectColor_active {
    border: 1px solid #ffda44!important;
    background-color: #fff4c7;
}

.timeShade {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background-color: rgba(0,0,0,0.5);
    z-index: 3;
}

.timeSelectCon {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 500rpx;
    overflow: hidden;
    background-color: #fff;
}

.timeSelect_l {
    float: left;
    width: 40%;
    height: 400rpx;
    text-align: center;
    overflow-y: auto;
    background-color: #f2f2f2;
}

.timeSelect_l>view,.timeSelect_r>view {
    line-height: 80rpx;
    color: #999;
}

.timeSelect_l>view.active {
    color: #333;
    background-color: #fff;
}

.timeSelect_r>view.active {
    color: #ffda44;
}

.timeSelect_r {
    float: left;
    width: 60%;
    height: 400rpx;
    text-align: center;
    overflow-y: auto;
}

.timeSelect_btn {
    height: 100rpx;
    line-height: 100rpx;
    font-size: 32rpx;
    text-align: center;
    border-top: 1rpx solid #e4e4e4;
}

.timeSelect_btn:active {
    background-color: #f2f2f2;
}

.faultListBG {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background-color: rgba(0,0,0,0.5);
    z-index: 999;
}

.faultListCon {
    position: absolute;
    width: 90%;
    left: 50%;
    top: 50%;
    max-height: 80%;
    transform: translateX(-50%) translateY(-50%);
    padding: 28rpx;
    border-radius: 8rpx;
    background-color: #fff;
}

.faultList_detailCon {
    max-height: 550rpx;
    overflow-y: scroll;
}

.faultListCon_title {
    text-align: center;
    font-size: 32rpx;
    margin-bottom: 20rpx;
}

.faultListRow {
    padding: 24rpx 0;
    border-bottom: 1px solid #e4e4e4;
}

.faultList_detailCon>view:last-child {
    border-bottom: none;
}

.faultListRow>view {
    font-size: 28rpx;
}

.faultListRow_l {
    float: left;
}

.faultListRow_r {
    float: right;
    color: #ffda44;
}

.faultListClose {
    position: absolute;
    right: -30rpx;
    top: -30rpx;
    width: 60rpx;
    height: 60rpx;
}

.guzhang_alertBox {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background-color: rgba(0,0,0,0.5);
    z-index: 4;
}

.guzhang_alertBox .alert_conBox {
    width: 640rpx;
    min-height: 200rpx;
    padding: 4%;
    background-color: white;
    position: fixed;
    top: 49%;
    left: 50%;
    margin-left: -320rpx;
    border-radius: 8px;
    transform: translateY(-50%);
}

.guzhang_alertBox .alert_conBox .title {
    text-align: center;
    font-size: 36rpx;
    color: #333;
    line-height: 50rpx;
}

.guzhang_alertBox .alert_conBox .title02 {
    text-align: center;
    font-size: 28rpx;
    color: #999;
    margin-bottom: 4%;
}

.guzhang_alertBox .alert_conBox .alert_con {
    width: 100%;
    font-size: 28rpx;
    border: 1px solid #efeff4;
    border-radius: 4px;
    line-height: 80rpx;
    padding: 0 5%;
    display: flex;
    margin-bottom: 3%;
}

.guzhang_alertBox .alert_conBox .alert_con.on {
    background-color: #fff4c7;
    border-color: #ffeda4;
}

.guzhang_alertBox .alert_conBox .alert_con .view01 {
    width: 77%;
}

.guzhang_alertBox .alert_conBox .alert_con .view02 {
    width: 23%;
    display: flex;
    justify-content: flex-end;
}

.guzhang_alertBox .alert_conBox .alert_bottonBox {
    width: 100%;
    display: flex;
    justify-content: space-between;
    margin-top: 6%;
}

.guzhang_alertBox .alert_conBox .alert_bottonBox .left {
    width: 48%;
}

.guzhang_alertBox .alert_conBox .alert_bottonBox .right {
    width: 48%;
}

button[type="default"] {
    color: #999;
    background-color: white;
}

button[type="primary"] {
    color: #333;
    background-color: #ffda44;
}

.guzhang_alertBox .right .button-hover {
    background-color: #ffda44;
}

.youhui_price {
    position: absolute;
    right: 8%;
    top: 50%;
    transform: translateY(-50%);
    color: #f74c31;
}

.youhui_price02 {
    position: absolute;
    right: 20%;
    top: 50%;
    transform: translateY(-50%);
    color: #f74c31;
}

.agreement {
    margin-top: 2%;
    color: #999;
}

.agreement_text {
    font-size: 28rpx;
}

checkbox {
    transform: scale(0.8);
}

.yuyue_text {
    color: red;
    position: absolute;
    right: 40rpx;
    top: 50%;
    transform: translateY(-50%);
    text-align: right;
}