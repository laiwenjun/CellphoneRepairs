page {
    background-color: #fff;
}

.top {
    position: relative;
    width: 100%;
    background-color: #f5ffff;
}

.top image {
    width: 100%;
}

.top_home {
    position: absolute;
    left: 0;
    top: 10rpx;
    padding: 10rpx 20rpx 10rpx 10rpx;
    border-top-right-radius: 40rpx;
    border-bottom-right-radius: 40rpx;
    color: #fff;
    font-size: 24rpx;
    background: rgba(0,0,0,0.3);
    z-index: 1;
}

.top_l {
    position: absolute;
    right: 30rpx;
    top: 10rpx;
    height: 50rpx;
    line-height: 50rpx;
    padding: 0 30rpx;
    color: #ffd700;
    border-radius: 25rpx;
    font-size: 24rpx;
    background: rgba(255,255,255,0.2);
}

.top_text {
    position: absolute;
    left: 0;
    bottom: 160rpx;
    width: 100%;
    text-align: center;
    font-size: 24rpx;
    color: #fff;
}

.top_btn {
    position: absolute;
    left: 50%;
    bottom: 40rpx;
    transform: translateX(-50%);
    font-size: 32rpx;
    height: 80rpx;
    line-height: 80rpx;
    padding: 0 50rpx;
    border-radius: 40rpx;
    background-color: #ffda44;
}

.main {
    background-color: #fff;
}

.main_top {
    padding-top: 30rpx;
    padding-left: 30rpx;
    color: #666;
    font-size: 32rpx;
}

.main_content {
    padding-top: 30rpx;
    padding-left: 48rpx;
}

.main_row {
    position: relative;
    padding-bottom: 50rpx;
    padding-left: 40rpx;
    padding-right: 30rpx;
    border-left: 4rpx solid #ffda44;
}

.circle {
    position: absolute;
    left: -18rpx;
    top: 0rpx;
    width: 36rpx;
    height: 36rpx;
    background-color: #fff;
    border: 4rpx solid #ffda44;
    border-radius: 100%;
    z-index: 1;
}

.main_time {
    font-size: 28rpx;
    color: #999;
}

.main_title {
    font-size: 36rpx;
    margin: 20rpx 0;
}

.main_tip,.main_tip2 {
    position: relative;
    font-size: 28rpx;
    color: #666;
    line-height: 50rpx;
}

.main_tip2 {
    padding-left: 40rpx;
}

.main_tip2::after {
    position: absolute;
    left: 10rpx;
    top: 24rpx;
    content: "";
    transform: translateY(-50%);
    width: 8rpx;
    height: 8rpx;
    border-radius: 100%;
    background-color: #333;
}

.mapImg {
    padding: 30rpx;
}

.mapImg image {
    width: 100%;
}

.bg {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.3);
    z-index: 2;
}

.footer_con {
    position: fixed;
    left: 0;
    bottom: 100rpx;
    width: 100%;
    padding-left: 30rpx;
    background-color: #fff;
    z-index: 3;
}

.footer_row {
    position: relative;
    padding: 10rpx 0;
    border-bottom: 1px solid #e4e4e4;
}

.footer_row>text {
    position: absolute;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
    color: #666;
    font-size: 28rpx;
}

.footer_row>input {
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    padding-left: 200rpx;
    font-size: 28rpx;
}

.footer_tip {
    color: #666;
    font-size: 28rpx;
    margin-top: 20rpx;
    margin-bottom: 40rpx;
}

.footer_select {
    padding-bottom: 30rpx;
}

.footer_select>text {
    padding: 10rpx 40rpx;
    border: 1rpx solid #e4e4e4;
    font-size: 28rpx;
    margin-right: 30rpx;
}

.footer_select>text.active {
    color: #ffda44;
    border-color: #ffda44;
}

.footer_btn {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    font-size: 32rpx;
    background-color: #ffda44;
    z-index: 4;
}

.footer_no {
    background-color: #ccc;
}

.myButton {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 9;
    width: 100%;
    height: 100%;
    opacity: 0;
}

.alert_bg {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background-color: rgba(0,0,0,0.3);
    z-index: 9;
}

.alert_img02 {
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    width: 75%;
    box-sizing: border-box;
    text-align: center;
}

.alert_img02 image {
    width: 100%;
}

.alert_img02_btn {
    margin: 5% auto;
    width: 100%;
    font-size: 16px;
}

.alert_box {
    width: 630rpx;
    height: 650rpx;
    padding-top: 20rpx;
    background-color: white;
    position: fixed;
    top: 45%;
    left: 50%;
    transform: translate(-50%,-50%);
    border-radius: 8px;
}

.alert_box .img {
    width: 600rpx;
}

.bottom_but {
    font-size: 32rpx;
    color: rgba(102,102,102,1);
    text-align: center;
    line-height: 86rpx;
    height: 86rpx;
    border-top: 1px solid #e4e4e4;
}