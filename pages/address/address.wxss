page {
    color: #333;
    background-color: #efeff4;
}

.addressSelectCon {
    margin: 20rpx 0;
    padding-left: 30rpx;
    background-color: #fff;
}

.addressSelectCon_row {
    position: relative;
    padding: 40rpx 0;
    border-bottom: 1rpx solid #e4e4e4;
}

.addressSelectCon_row_l,.addressSelectCon_row_l_active {
    position: absolute;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
    width: 40rpx;
    height: 40rpx;
    border-radius: 100%;
    border: 1rpx solid #999;
}

.addressSelectCon_row_l_active {
    border: none;
    background-color: #89ca12;
}

.addressSelectCon_content {
    padding: 10rpx 50rpx 0 80rpx;
}

.addressSelectCon_content view:first-child {
    font-size: 32rpx;
    font-weight: 700;
    margin-bottom: 10rpx;
    max-width: 80%;
}

.addressSelectCon_content view:nth-of-type(2) {
    color: #999;
    font-size: 24rpx;
    max-width: 80%;
}

.arrows {
    position: absolute;
    right: 50rpx;
    top: 50%;
    width: 28rpx;
    height: 28rpx;
    border-width: 1rpx;
    border-color: #999 #999 transparent transparent;
    border-style: solid;
    transform: rotate(45deg) translateY(-50%);
}

.del {
    position: absolute;
    right: 30rpx;
    top: 50%;
    width: 60rpx;
    height: 60rpx;
    transform: translateY(-50%);
    z-index: 2;
}

.edit {
    position: absolute;
    right: 100rpx;
    top: 50%;
    width: 60rpx;
    height: 60rpx;
    transform: translateY(-50%);
}

.jiahao {
    position: absolute;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 40rpx;
    font-weight: 700;
    color: #999;
}