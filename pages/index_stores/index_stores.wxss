page {
    background-color: #f5f5f5;
}

swiper {
    height: 360rpx;
}

swiper-item image {
    width: 100%;
    height: 100%;
}

.getIphoneCon {
    position: relative;
    padding: 20rpx 30rpx;
    width: 94%;
    margin: -60rpx auto 20rpx;
    background-color: #fff;
    border-radius: 8rpx;
    height: 160rpx;
    border: 1px solid #e4e4e4;
}

.getIphoneCon_l {
    position: absolute;
    top: 50%;
    left: 20rpx;
    transform: translateY(-50%);
    width: 100rpx;
    height: 100rpx;
}

.getIphoneCon_l>image {
    width: 100%;
    height: 100%;
}

.getIphoneCon_m {
    position: absolute;
    top: 50%;
    left: 150rpx;
    transform: translateY(-50%);
}

.getIphoneCon_m_t {
    font-size: 24rpx;
}

.getIphoneCon_m_b {
    font-size: 32rpx;
}

.getIphoneCon_r {
    position: absolute;
    top: 50%;
    right: 30rpx;
    transform: translateY(-50%);
    padding: 16rpx 30rpx;
    border: 1rpx solid #e4e4e4;
    border-radius: 8rpx;
    font-size: 28rpx;
    color: #333;
}

.hover_bg {
    background-color: #f5f5f5!important;
}

.hover_bg_on {
    background-color: #ffca2b!important;
}

.model_title {
    display: flex;
    width: 100%;
    box-sizing: border-box;
    padding: 2.5% 3%;
    font-size: 32rpx;
    background-color: white;
}

.model_title .text {
    display: flex;
    width: 77%;
    text-align: right;
    line-height: 32rpx;
    vertical-align: middle;
    align-self: center;
    box-sizing: border-box;
    padding-left: 52rpx;
    position: relative;
    font-size: 28rpx;
    color: #999;
}

.model_title .img {
    width: 32rpx;
    height: 32rpx;
    margin-right: 16rpx;
    position: absolute;
    top: 50%;
    margin-top: -16rpx;
    left: 0;
}

.model_title b {
    font-weight: 600;
    color: #333;
}

.model_title .chooseModel {
    display: flex;
    width: 23%;
    text-align: right;
    color: #5d9cec;
    line-height: 36rpx;
    vertical-align: middle;
    font-size: 28rpx;
}

.chooseFailure {
    border: 1px solid #e4e4e4;
    background-color: #fff;
}

.chooseFailureBox {
    width: 100%;
    padding: 0% 3%;
    box-sizing: border-box;
}

.chooseFailure .chooseFailure_div {
    position: relative;
    float: left;
    width: 33.33%;
    height: 120rpx;
    border-right: 1px solid #e4e4e4;
    border-bottom: 1px solid #e4e4e4;
    padding: 20rpx;
    background-color: white;
    box-sizing: border-box;
}

.chooseFailure>view:nth-of-type(3n) {
    border-right: none;
}

.chooseFailure>view:last-child {
    border-bottom: none;
}

.selected {
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    right: 0;
    bottom: 0;
    display: none;
}

.chooseFailure .chooseFailure_div.on,.othersFailure.on {
    border: 1px solid #fad502;
}

.chooseFailure .chooseFailure_div.on .selected,.othersFailure.on .selected {
    display: block;
}

.buttonBox {
    width: 100%;
    box-sizing: border-box;
    position: fixed;
    bottom: 0;
    background-color: #efeff4;
    z-index: 4;
}

.button {
    background-color: #ffda44;
    font-size: 32rpx;
    font-weight: 500;
    color: #333;
}

.button-hover {
    color: #eee;
}

.failure {
    position: absolute;
    top: 50%;
    left: 11%;
    padding-left: 60rpx;
    line-height: 44rpx;
    color: #333;
    font-size: 28rpx;
    margin-top: -22rpx;
}

.detail {
    display: block;
    width: 100%;
    padding-left: 60rpx;
    line-height: 44rpx;
    font-size: 24rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #999;
}

.failure b {
    float: right;
}

.failure .img {
    position: absolute;
    width: 38rpx;
    height: 38rpx;
    left: 10rpx;
    top: 50%;
    transform: translateY(-50%);
}

.othersFailure {
    float: left;
    width: 100%;
    border: 1px solid #ddd;
    background-color: white;
    height: 200rpx;
    margin-top: 3%;
    position: relative;
    padding: 20rpx;
}

.othersFailure_title {
    width: 100%;
    line-height: 48rpx;
    border-bottom: 1px solid #ddd;
    position: relative;
    font-size: 30rpx;
    color: #333;
    padding-left: 60rpx;
    padding-bottom: 18rpx;
}

.othersFailure_title .img {
    position: absolute;
    width: 38rpx;
    height: 38rpx;
    left: 10rpx;
    top: 6rpx;
}

.inputBox {
    padding: 18rpx 0 20rpx;
}

.inputBox input {
    padding: 10rpx 0 10rpx 16rpx;
    font-size: 28rpx;
    box-shadow: 0 0 0 100px #f0f0f0 inset;
    border-radius: 4px;
}

.replaceModelBox {
    background: rgba(67,67,67,0.6);
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 3;
}

.replaceModel {
    z-index: 4;
    position: fixed;
    bottom: 200rpx;
    width: 90%;
    background-color: white;
    text-align: center;
    left: 5%;
}

.confirm {
    width: 100%;
    position: absolute;
    bottom: 20rpx;
    background-color: white;
    font-size: 40rpx;
    line-height: 80rpx;
    text-align: center;
}

.confirmHover {
    background-color: #efeff4;
}

.flex {
    display: flex;
    line-height: 80rpx;
}

.flex view {
    width: 50%;
    display: flex;
    justify-content: center;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.alertBox {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background-color: rgba(51,51,51,0.3);
    z-index: 4;
    display: none;
}

.alertBox .alert {
    position: fixed;
    left: 50%;
    width: 560rpx;
    top: 50%;
    margin-left: -280rpx;
    background-color: white;
    z-index: 5;
    transform: translateY(-50%);
}

.alertBox .alert .alert_div {
    text-align: center;
    line-height: 80rpx;
    font-size: 32rpx;
    border-bottom: 1px solid #efeff4;
}

.alertBox.active {
    display: block!important;
}

.chooseFailureBotton {
    margin-top: 80rpx;
    width: 100%;
    box-sizing: border-box;
    padding: 10% 20%;
}

.chooseFailureBotton_title {
    text-align: center;
    font-size: 32rpx;
    margin-bottom: 10%;
}

.top {
    position: relative;
}

.top_img {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 580rpx;
}

.icoBox {
    position: absolute;
    top: 22%;
    left: 50%;
    background-color: white;
    border-radius: 50%;
    transform: translateX(-50%);
    height: 220rpx;
    width: 220rpx;
}

.icoBOX02 {
    border: 4px solid #ffda44;
    border-radius: 50%;
    height: 210rpx;
    width: 210rpx;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    overflow: hidden;
}

.icoBox .model_img {
    width: 140rpx;
    margin-top: -70rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -70rpx;
    border-radius: 16rpx;
}

.model_selBox {
    position: absolute;
    bottom: 60rpx;
    width: 100%;
    text-align: center;
    z-index: 2;
}

.model_selBox .view01 {
    font-size: 40rpx;
}

.model_selBox .view02 {
    font-size: 28rpx;
    margin-top: 20rpx;
}

.model_selBox .view02 text {
    display: inline-block;
    padding: 6rpx 20rpx;
    border-radius: 50rpx;
    background-color: #ff9300;
    color: white;
    line-height: 50rpx;
}

.model_selBox .view02 text .text_b {
    font-size: 36rpx;
}

.myBox {
    width: 120rpx;
    height: 96rpx;
    background-color: white;
    border-radius: 50rpx 0 0 50rpx;
    position: fixed;
    z-index: 2;
    right: 0;
    top: 4%;
    overflow: hidden;
}

.myButton {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 9;
    width: 100%;
    height: 100%;
    opacity: 0;
}

.myBox .myImg {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
    position: absolute;
    left: 8rpx;
    top: 50%;
    transform: translateY(-50%);
    overflow: hidden;
}

.myBox .myImg image {
    width: 80rpx;
    height: 80rpx;
}

.bottomBox {
    position: relative;
}

.buttonBox .bottom_butBox {
    position: absolute;
    bottom: 0;
    width: 100%;
    border-top: 1rpx solid #f5f5f5;
}

.buttonBox .bottom_butBox .left {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 70%;
    padding-left: 180rpx;
    height: 111rpx;
    line-height: 111rpx;
    background-color: #fff;
    color: #999;
    font-size: 30rpx;
}

.buttonBox .bottom_butBox .right {
    position: absolute;
    right: 0;
    bottom: 0;
    width: 30%;
    text-align: center;
    height: 111rpx;
    line-height: 111rpx;
    background-color: #ccc;
    font-size: 36rpx;
}

.bottom_img {
    width: 110rpx;
    height: 110rpx;
    position: absolute;
    margin-top: -24rpx;
    left: 20rpx;
    z-index: 3;
    top: -4rpx;
    transition: all 0.5s;
}

.bottom_number {
    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    position: absolute;
    margin-top: -24rpx;
    font-size: 24rpx;
    left: 100rpx;
    z-index: 4;
    top: -12rpx;
    background-color: red;
    text-align: center;
    border-radius: 50%;
    color: white;
    transition: all 0.5s;
}

.guzhang_box.on .bottom_number {
    top: -120rpx;
}

.bottom_img.on {
    display: none;
}

.block_view {
    width: 100%;
    height: 111rpx;
    z-index: 2;
    position: absolute;
    bottom: 0;
    left: 0;
}

.guzhang_box {
    width: 100%;
    background-color: white;
    z-index: -1;
    position: absolute;
    bottom: 0;
    left: 0;
    padding-bottom: 110rpx;
}

.guzhang_title {
    background-color: #f9f9f9;
    height: 88rpx;
    line-height: 88rpx;
    padding: 0 3%;
    font-size: 34rpx;
    color: #333;
    position: relative;
}

.guzhang_title:after {
    content: "";
    width: 0;
    height: 0;
    border-left: 16rpx solid transparent;
    border-right: 16rpx solid transparent;
    border-bottom: 32rpx solid #f9f9f9;
    position: absolute;
    left: 58rpx;
    top: -18rpx;
}

.guzhang_text {
    position: relative;
    width: 97%;
    line-height: 110rpx;
    border-bottom: 1px solid #e4e4e4;
    margin-left: 3%;
    height: 110rpx;
}

.guzhang_text:last-of-type {
    border-bottom: none;
}

.guzhang_text text {
    color: #666;
    font-size: 28rpx;
}

.guzhang_price {
    position: absolute;
    right: 50rpx;
    top: 50%;
    color: #999!important;
    transform: translateX(-50%) translateY(-50%);
}

.guzhang_imgBox {
    display: inline-block;
    width: 48rpx;
    height: 48rpx;
    position: absolute;
    right: 0rpx;
    border: 1px solid #e4e4e4;
    border-radius: 50%;
    top: 50%;
    transform: translateX(-50%) translateY(-50%);
}

.guzhang_img {
    width: 20rpx;
    height: 20rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translateX(-50%) translateY(-50%);
}

.guzhang_hide {
    transition: all 0.5s;
}

.guzhang_box.on .guzhang_hide {
    padding-bottom: 0;
}

.guzhang_box.on .bottom_img {
    top: -110rpx;
}

.guzhang_box.on .left {
    padding-left: 3%;
}

.guzhang_box .right.on {
    background-color: #ffda44;
    color: black;
}

.buttonBox .bottom_butBox .left02 {
    width: 70%;
    padding-left: 160rpx;
    height: 110rpx;
    background-color: #fff;
    color: gray;
    transition: all 0.5s;
}

.buttonBox .bottom_butBox .left02 .view01 {
    font-size: 36rpx;
    color: #ffda44;
    padding-top: 10rpx;
}

.buttonBox .bottom_butBox .left02 .view02 {
    font-size: 24rpx;
    color: #999;
}

.guzhang_box.on .left02 {
    padding-left: 3%;
}

.guzhang_block {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background-color: rgba(0,0,0,0.5);
    z-index: 3;
}

.guzhang_alertBox {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background-color: rgba(0,0,0,0.5);
    z-index: 4;
}

.guzhang_alertBox .alert_conBox {
    width: 640rpx;
    min-height: 200rpx;
    padding: 4%;
    background-color: white;
    position: fixed;
    top: 49%;
    left: 50%;
    margin-left: -320rpx;
    border-radius: 8px;
    transform: translateY(-50%);
}

.guzhang_alertBox .alert_conBox .title {
    text-align: center;
    font-size: 36rpx;
    color: #333;
    line-height: 50rpx;
}

.guzhang_alertBox .alert_conBox .title02 {
    text-align: center;
    font-size: 28rpx;
    color: #999;
    margin-bottom: 4%;
}

.guzhang_alertBox .alert_conBox .alert_con {
    position: relative;
    width: 100%;
    font-size: 28rpx;
    border: 1px solid #efeff4;
    border-radius: 4px;
    line-height: 80rpx;
    padding: 0 20rpx;
    margin-bottom: 3%;
}

.guzhang_alertBox .alert_conBox .alert_con02 {
    position: relative;
    width: 100%;
    font-size: 28rpx;
    border-radius: 4px;
    line-height: 40rpx;
    padding: 0 20rpx;
    margin-bottom: 3%;
    color: #999;
}

.guzhang_alertBox .alert_conBox .alert_con.on {
    background-color: #fff4c7;
    border-color: #ffeda4;
}

.guzhang_alertBox .alert_conBox .alert_con .view01 {
    width: 77%;
}

.guzhang_alertBox .alert_conBox .alert_con .view02 {
    position: absolute;
    right: 20rpx;
    top: 50%;
    transform: translateY(-50%);
}

.guzhang_alertBox .alert_conBox .alert_bottonBox {
    margin-top: 20rpx;
}

.guzhang_alertBox .alert_conBox .alert_bottonBox .left {
    float: left;
    width: 48%;
}

.guzhang_alertBox .alert_conBox .alert_bottonBox .right {
    float: right;
    width: 48%;
}

button[type="default"] {
    color: #999;
    background-color: white;
}

button[type="primary"] {
    color: #333;
    background-color: #ffda44;
}

.guzhang_alertBox .right .button-hover {
    background-color: #ffda44;
}

.fixed_img {
    width: 140rpx;
    height: 140rpx;
    position: fixed;
    right: 2%;
    bottom: 10%;
}

.fixed_img>image {
    width: 140rpx;
    height: 140rpx;
}

.fixed_img .navigator,swiper-item .navigator {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 1;
    opacity: 0;
}

.activityCon {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.5);
    z-index: 99;
}

.activityMian {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translateX(-50%) translateY(-50%);
    text-align: center;
}

.activityMian_img1 {
    width: 500rpx;
    height: 610rpx;
}

.activityMian_img2 {
    width: 72rpx;
    height: 72rpx;
    margin-top: 40rpx;
}